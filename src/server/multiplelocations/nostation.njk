{% extends 'layouts/page.njk' %}
{% set displayBreadcrumbs = false %}
{% set title = "We could not find" + " " + data['q'] %}

{% block content %}
<div class="govuk-grid-row">
  <div class="govuk-grid-column-two-thirds">
    <h1 class="govuk-heading-l odd-page">{{paragraph.a}} {{ locationMiles }} {{paragraph.b}} '{{ searchLocation }}'</h1>
     {% if locationMiles !== 50 %} 
    <p class="govuk-body">{{paragraph.d}}</p>

    <ul class="govuk-list govuk-list--bullet">
      <li>{{paragraph.e}}</li>
      <li>{{paragraph.f}}</li>     
    </ul>
    {% endif %}
    <p class="govuk-body"><a href='/search-location'>{{paragraph.g}}</a></p>
   
  </div>
</div>
{% endblock %}
